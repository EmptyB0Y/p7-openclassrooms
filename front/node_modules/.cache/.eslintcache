e-frame'>\r\n            <img id=\"profile-picture\" src={profile.pictureUrl}></img>\r\n          </div>\r\n          <div id='description'>\r\n            <h3>{profile.description}</h3>\r\n          </div>\r\n            {editSecondaryInfosElement}        \r\n        </div>\r\n      </div>\r\n      <div id='posts-frame'>\r\n        {postsElement}\r\n      </div>\r\n    </div>\r\n        )\r\n}\r\n\r\nconst handleClick = (id,e) => {\r\n  e.preventDefault();\r\n  let x = document.getElementById(\"comments-\"+id);\r\n  if (x.style.display === \"none\") {\r\n      x.style.display = \"block\";\r\n  } else {\r\n      x.style.display = \"none\";\r\n  }\r\n}",["121","122"],"C:\\Users\\LILKETRU\\Desktop\\openclassrooms\\P7\\Groupomania\\front\\src\\services\\profiles.service.js",[],[],["123","124"],"C:\\Users\\LILKETRU\\Desktop\\openclassrooms\\P7\\Groupomania\\front\\src\\components\\AddPost.js",["125"],[],{"ruleId":"126","severity":1,"message":"127","line":12,"column":9,"nodeType":"128","endLine":12,"endColumn":48},{"ruleId":"126","severity":1,"message":"127","line":14,"column":13,"nodeType":"128","endLine":14,"endColumn":54},{"ruleId":"126","severity":1,"message":"127","line":15,"column":13,"nodeType":"128","endLine":15,"endColumn":54},{"ruleId":"126","severity":1,"message":"127","line":25,"column":17,"nodeType":"128","endLine":25,"endColumn":56},{"ruleId":"126","severity":1,"message":"127","line":27,"column":21,"nodeType":"128","endLine":27,"endColumn":59},{"ruleId":"126","severity":1,"message":"127","line":28,"column":21,"nodeType":"128","endLine":28,"endColumn":62},{"ruleId":"129","severity":1,"message":"130","line":4,"column":31,"nodeType":"131","messageId":"132","endLine":4,"endColumn":37},{"ruleId":"129","severity":1,"message":"133","line":4,"column":39,"nodeType":"131","messageId":"132","endLine":4,"endColumn":50},{"ruleId":"126","severity":1,"message":"127","line":35,"column":31,"nodeType":"128","endLine":35,"endColumn":43},{"ruleId":"134","severity":1,"message":